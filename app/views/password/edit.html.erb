<h1>Edit Password</h1>
<%= form_with model: @user, url: password_path, method: :patch do |f| %>

    <%= f.label :password %>
    <%= f.password_field :password_digest %><br><br>

    <%= f.label :Confirm_Password %>
    <%= f.password_field :password_confirmation %><br><br>

  
    <%= f.submit "changePassword"%>
  
<% end %>