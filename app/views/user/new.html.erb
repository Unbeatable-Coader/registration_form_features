<h1>Registration Form<h1>
<%= form_for @user, url: user_path(@user), method: :post do |f| %>



    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :Confirm_Password %>
    <%= f.password_field :password_confirmation %>

  
    <%= f.submit "Sign Up" %><br>
  
    <%= link_to "log-in", login_path %>
<% end %>