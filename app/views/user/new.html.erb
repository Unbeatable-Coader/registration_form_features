<h1>Registration Form<h1>
<%= form_for @user, url: user_path(@user), method: :post do |f| %>



    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :email %>
    <%= f.email_field :email %><br><br>

    <%= f.label :password %>
    <%= f.password_field :password %><br><br>

    <%= f.label :Confirm_Password %>
    <%= f.password_field :password_confirmation %><br><br>

  
    <%= f.submit "Sign Up" %><&nbsp>
  
    <%= link_to "log-in", login_path %>
<% end %>